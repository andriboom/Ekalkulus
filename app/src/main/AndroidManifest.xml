<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="tb.um.kalkulus2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_learning"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_learning_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup">
        <activity android:name=".PilihLatihanActivity" />
        <activity android:name=".PilihMateriActivity" />
        <activity android:name=".KunciJawabanActivity" />
        <activity android:name=".LihatHasilActivity" />
        <activity android:name=".DetailTheoryActivity" />
        <activity android:name=".TheoryActivity" />
        <activity android:name=".InformationActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
    </application>

</manifest>